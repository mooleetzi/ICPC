{
    "version": "0.2.0",
    "configurations": [{
        "name": "(gdb) Launch",
        "preLaunchTask": "build",
        "type": "cppdbg",
        "request": "launch",
        "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": true,
        "MIMode": "gdb",
        "miDebuggerPath": "F:/Program Files (x86)/CodeBlocks/MinGW/bin/gdb32.exe", // GDB的路径，注意替换成自己的路径
        "setupCommands": [{
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        }]
    }]
}